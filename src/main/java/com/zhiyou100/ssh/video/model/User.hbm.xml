<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zhiyou100.ssh.video.model">
   <class name="User" table="user">
        <id name="id">
           <generator class="native"></generator>
        </id>
        <property name="nickName" column="nick_name"></property>
        <property name="sex" column="sex"></property>
        <property name="birthday" column="birthday"></property>
        <property name="email" column="email"></property>
        <property name="province" column="province"></property>
        <property name="city" column="city"></property>
        <property name="headUrl" column="head_url"></property>
        <property name="password" column="password"></property>
        <property name="insertTime" column="insert_time"></property>
        <property name="updateTime" column="update_time"></property>
        <property name="captcha" column="captcha"></property>
        
        
        
   </class>

</hibernate-mapping>